<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite configfailurepolicy="continue" name="RegressionSuite">
    <listeners>
        <listener class-name="com.auto.testng.TestListener"/>
    </listeners>

    <test name="Regression tests">
        <parameter name="platform" value="chrome"/>
        <classes>
            <class name="com.auto.test.tadashboard.LoginPositiveTest"/>
            <class name="com.auto.test.tadashboard.LoginNegativeTest"/>
            <class name="com.auto.test.tadashboard.DashboardPositiveTest"/>
            <class name="com.auto.test.tadashboard.DashboardNegativeTest"/>
            <class name="com.auto.test.tadashboard.panel.PanelNegativeTest"/>
            <class name="com.auto.test.tadashboard.panel.PanelPositiveTest"/>
        </classes>
    </test>

</suite>
